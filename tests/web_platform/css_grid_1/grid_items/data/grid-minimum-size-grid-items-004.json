{
    "assert": "Checks that minimum size for grid items is the specified size regardless of the content size.",
    "test_case": {
        "style": {
            "margin_top": "8px",
            "margin_bottom": "8px",
            "margin_left": "8px",
            "display": "block",
            "margin_right": "8px",
            "unicode_bidi": "embed"
        },
        "children": [
            {
                "style": {
                    "width": "100px",
                    "position": "absolute",
                    "height": "100px",
                    "z_index": "-1",
                    "display": "block",
                    "background_color": "red",
                    "unicode_bidi": "embed"
                },
                "tag": "DIV"
            },
            {
                "style": {
                    "display": "grid",
                    "width": "10px",
                    "height": "10px",
                    "unicode_bidi": "embed"
                },
                "children": [
                    {
                        "style": {
                            "display": "block",
                            "width": "100px",
                            "height": "100px",
                            "background_color": "green",
                            "unicode_bidi": "embed"
                        },
                        "children": [
                            {
                                "style": {
                                    "display": "block",
                                    "width": "50px",
                                    "height": "50px",
                                    "unicode_bidi": "embed"
                                },
                                "tag": "DIV"
                            }
                        ],
                        "tag": "DIV"
                    }
                ],
                "tag": "DIV"
            }
        ],
        "tag": "BODY"
    },
    "help": [
        "http://www.w3.org/TR/css-grid-1/#min-size-auto"
    ],
    "matches": "../../reference/ref-filled-green-100px-square.xht"
}
