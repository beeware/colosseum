{
    "assert": "Checks that automatic minimum size gets clamped, so the grid item doesn't overflow the fixed size area.",
    "test_case": {
        "style": {
            "margin_top": "8px",
            "margin_bottom": "8px",
            "margin_left": "8px",
            "display": "block",
            "margin_right": "8px",
            "unicode_bidi": "embed"
        },
        "children": [
            {
                "style": {
                    "width": "100px",
                    "position": "absolute",
                    "height": "100px",
                    "z_index": "-1",
