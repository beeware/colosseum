{
    "matches": "../../reference/ref-filled-green-100px-square.xht",
    "assert": "This test checks that 'inline-grid' value for 'display' property generates an inline level containing box.",
    "test_case": {
        "tag": "BODY",
        "style": {
            "margin_left": "8px",
            "margin_bottom": "8px",
            "display": "block",
            "unicode_bidi": "embed",
            "margin_right": "8px",
            "margin_top": "8px"
        },
        "children": [
            {
                "tag": "DIV",
                "style": {
                    "width": "100px",
                    "z_index": "-1",
                    "background_color": "red",
                    "display": "block",
                    "unicode_bidi": "embed",
                    "height": "100px",
                    "position": "absolute"
                }
            },
            {
                "tag": "DIV",
                "style": {
                    "display": "inline-grid",
                    "unicode_bidi": "embed",
                    "height": "100px",
                    "width": "50px",
                    "background_color": "green"
                }
            },
            {
                "tag": "DIV",
                "style": {
                    "display": "inline-grid",
                    "unicode_bidi": "embed",
                    "height": "100px",
                    "width": "50px",
                    "background_color": "green"
                }
            }
        ]
    },
    "help": [
        "http://www.w3.org/TR/css-grid-1/#grid-containers"
    ]
}