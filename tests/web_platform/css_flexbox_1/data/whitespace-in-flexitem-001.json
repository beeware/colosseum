{
    "help": [
        "https://drafts.csswg.org/css-flexbox/#flex-items"
    ],
    "matches": "whitespace-in-flexitem-001-ref.html",
    "assert": "Whitespace-only nodes in a flexbox should not\nbe rendered and not influence justify-content, even in the presence of\nwhite-space: pre",
    "test_case": {
        "style": {
            "margin_right": "8px",
            "margin_bottom": "8px",
            "display": "block",
            "margin_top": "8px",
            "unicode_bidi": "embed",
            "margin_left": "8px"
        },
        "tag": "BODY",
        "children": [
            {
                "style": {
                    "white_space": "pre",
                    "justify_content": "space-around",
                    "unicode_bidi": "embed",
                    "display": "flex"
                },
                "tag": "DIV",
                "children": [
                    {
                        "style": {
                            "background_color": "salmon",
                            "flex_grow": "0",
                            "width": "30px",
                            "background_repeat_y": "initial",
                            "flex_shrink": "0",
                            "background_origin": "initial",
                            "flex_basis": "auto",
                            "background_position_y": "initial",
                            "unicode_bidi": "embed",
                            "background_size": "initial",
                            "background_clip": "initial",
                            "display": "block",
                            "background_attachment": "initial",
                            "background_image": "initial",
                            "background_repeat_x": "initial",
                            "background_position_x": "initial"
                        },
                        "tag": "DIV"
                    }
                ]
            },
            {
                "style": {
                    "white_space": "pre",
                    "unicode_bidi": "embed",
                    "display": "flex"
                },
                "tag": "DIV",
                "children": [
                    {
                        "style": {
                            "font_weight": "bolder"
                        },
                        "tag": "B",
                        "children": [
                            {
                                "style": {},
                                "text": "foo"
                            }
                        ]
                    },
                    {
                        "style": {
                            "font_weight": "bolder"
                        },
                        "tag": "B",
                        "children": [
                            {
                                "style": {},
                                "text": "bar"
                            }
                        ]
                    }
                ]
            }
        ]
    }
}
