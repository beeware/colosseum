{
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#dis-pos-flo"
    ],
    "matches": "../reference/ref-filled-green-100px-square.xht",
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "style": {
                            "left": "auto",
                            "width": "100px",
                            "display": "block",
                            "unicode_bidi": "embed",
                            "background_color": "green",
                            "height": "100px",
                            "float": "right",
                            "position": "absolute",
                            "top": "auto"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "100px",
                            "display": "block",
                            "unicode_bidi": "embed",
                            "background_color": "red",
                            "height": "100px",
                            "position": "static"
                        },
                        "tag": "div"
                    }
                ],
                "style": {
                    "unicode_bidi": "embed",
                    "height": "100px",
                    "position": "relative",
                    "width": "auto",
                    "display": "block"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_right": "8px",
            "margin_top": "8px",
            "display": "block",
            "unicode_bidi": "embed",
            "margin_bottom": "8px",
            "margin_left": "8px"
        },
        "tag": "body"
    },
    "assert": "Float is computed to 'none' when 'position: absolute' is specified."
}