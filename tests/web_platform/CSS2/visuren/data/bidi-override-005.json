{
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-unicode-bidi",
        "http://www.w3.org/TR/CSS21/visuren.html#direction"
    ],
    "matches": null,
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "text": "cba",
                        "style": {}
                    },
                    {
                        "children": [
                            {
                                "text": "abc",
                                "style": {}
                            }
                        ],
                        "style": {
                            "unicode_bidi": "embed",
                            "display": "block"
                        },
                        "tag": "div"
                    }
                ],
                "style": {
                    "unicode_bidi": "bidi-override",
                    "direction": "rtl",
                    "display": "block"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_right": "8px",
            "margin_top": "8px",
            "display": "block",
            "unicode_bidi": "embed",
            "margin_bottom": "8px",
            "margin_left": "8px"
        },
        "tag": "body"
    },
    "assert": "bidi-override should be applied to inline-level descendants but not block-level descendants"
}
