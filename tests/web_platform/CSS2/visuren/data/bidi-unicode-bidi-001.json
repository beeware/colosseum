                    "margin_left": "0",
                    "direction": "ltr"
                },
                "tag": "p"
            },
            {
                "children": [
                    {
                        "text": "a b,",
                        "style": {}
                    },
                    {
                        "children": [
                            {
                                "text": "d c",
                                "style": {}
                            }
                        ],
                        "style": {
                            "unicode_bidi": "bidi-override",
                            "direction": "rtl"
                        },
                        "tag": "span"
                    },
                    {
                        "text": ".",
                        "style": {}
                    }
                ],
                "style": {
                    "margin_right": "0",
                    "margin_top": "1.12em",
                    "display": "block",
                    "unicode_bidi": "embed",
                    "margin_bottom": "1.12em",
                    "margin_left": "0"
                },
                "tag": "p"
            }
        ],
        "style": {
            "margin_right": "8px",
            "margin_top": "8px",
            "display": "block",
            "unicode_bidi": "embed",
            "margin_bottom": "8px",
            "margin_left": "8px"
