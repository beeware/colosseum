{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#the-height-property",
        "http://www.w3.org/TR/CSS21/visufx.html#overflow"
    ],
    "test_case": {
        "tag": "body",
        "style": {
            "display": "block",
            "unicode_bidi": "embed",
            "margin_right": "8px",
            "margin_left": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px"
        },
        "children": [
            {
                "tag": "div",
                "style": {
                    "display": "list-item",
                    "font_weight": "normal",
                    "font_size": "2em",
                    "overflow_x": "visible",
                    "color": "green",
                    "unicode_bidi": "embed",
                    "line_height": "1",
                    "font_style": "normal",
                    "height": "0px",
                    "margin_left": "1.25em",
                    "overflow_y": "visible",
                    "font_family": "serif",
                    "font_variant_caps": "normal"
                },
                "children": [
                    {
                        "text": "PASS",
                        "style": {}
                    }
                ]
            },
            {
                "tag": "ul",
                "style": {
                    "display": "block",
                    "unicode_bidi": "embed",
                    "height": "auto",
                    "margin_right": "0",
                    "margin_left": "40px",
                    "overflow_x": "visible",
                    "overflow_y": "visible",
                    "margin_top": "3em",
                    "margin_bottom": "1.12em"
                },
                "children": [
                    {
                        "tag": "li",
                        "style": {
                            "line_height": "1",
                            "display": "list-item",
                            "font_weight": "normal",
                            "font_size": "2em",
                            "color": "green",
                            "height": "0px",
                            "overflow_x": "visible",
                            "font_family": "serif",
                            "font_variant_caps": "normal",
                            "overflow_y": "visible",
                            "font_style": "normal"
                        },
                        "children": [
                            {
                                "text": "PASS",
                                "style": {}
                            }
                        ]
                    }
                ]
            },
            {
                "tag": "ul",
                "style": {
                    "display": "block",
                    "unicode_bidi": "embed",
                    "height": "0px",
                    "margin_right": "0",
                    "margin_left": "40px",
                    "overflow_x": "visible",
                    "overflow_y": "visible",
                    "margin_top": "3em",
                    "margin_bottom": "1.12em"
                },
                "children": [
                    {
                        "tag": "li",
                        "style": {
                            "line_height": "1",
                            "display": "list-item",
                            "font_weight": "normal",
                            "font_size": "2em",
                            "color": "green",
                            "height": "0px",
                            "overflow_x": "visible",
                            "font_family": "serif",
                            "font_variant_caps": "normal",
                            "overflow_y": "visible",
                            "font_style": "normal"
                        },
                        "children": [
                            {
                                "text": "PASS",
                                "style": {}
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "matches": "height-applies-to-010a-ref.xht",
    "assert": "If height of content exceeds the set height of a block-level non-replaced element in normal flow (like a list-item element such as in this test), then the content should overflow according to the 'overflow' property."
}