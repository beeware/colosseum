{
    "help": [
        "http://www.w3.org/TR/CSS22/visudet.html#min-max-heights",
        "http://www.w3.org/TR/CSS22/changes.html#s.10.7"
    ],
    "test_case": {
        "tag": "body",
        "style": {
            "display": "block",
            "background_repeat_y": "initial",
            "font_size": "14px",
            "background_attachment": "initial",
            "background_size": "initial",
            "background_repeat_x": "initial",
            "color": "black",
            "unicode_bidi": "embed",
            "background_position_x": "initial",
            "background_position_y": "initial",
            "background_image": "initial",
            "margin_right": "8px",
            "margin_left": "8px",
            "background_clip": "initial",
            "background_origin": "initial",
            "background_color": "white",
            "margin_top": "8px",
            "margin_bottom": "8px"
        },
        "children": [
            {
                "tag": "p",
                "style": {
                    "display": "block",
                    "unicode_bidi": "embed",
                    "margin_right": "0",
                    "margin_left": "0",
                    "margin_top": "1.12em",
                    "margin_bottom": "1.12em"
                },
                "children": [
                    {
                        "tag": "span",
                        "style": {
                            "display": "inline-block",
                            "background_repeat_y": "initial",
                            "background_attachment": "initial",
                            "background_image": "initial",
                            "background_repeat_x": "initial",
                            "background_position_x": "initial",
                            "background_position_y": "initial",
                            "height": "70px",
                            "background_size": "initial",
                            "background_clip": "initial",
                            "background_origin": "initial",
                            "background_color": "green",
                            "min_height": "140px"
                        },
                        "children": [
                            {
                                "tag": "span",
                                "style": {
                                    "display": "inline-block",
                                    "background_repeat_y": "initial",
                                    "background_attachment": "initial",
                                    "background_image": "initial",
                                    "background_repeat_x": "initial",
                                    "background_position_x": "initial",
                                    "background_position_y": "initial",
                                    "height": "inherit",
                                    "background_size": "initial",
                                    "width": "70px",
                                    "background_clip": "initial",
                                    "background_origin": "initial",
                                    "background_color": "rgb(102, 102, 255)"
                                }
                            },
                            {
                                "tag": "span",
                                "style": {
                                    "display": "inline-block",
                                    "background_repeat_y": "initial",
                                    "background_attachment": "initial",
                                    "background_image": "initial",
                                    "background_repeat_x": "initial",
                                    "background_position_x": "initial",
                                    "background_position_y": "initial",
                                    "height": "70px",
                                    "background_size": "initial",
                                    "width": "70px",
                                    "background_clip": "initial",
                                    "background_origin": "initial",
                                    "background_color": "rgb(170, 170, 255)"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "matches": "height-computed-001-ref.xht",
    "assert": "If the resulting height is smaller than 'min-height', the rules above are applied again, but this time using the value of 'min-height' as the computed value for 'height'. Note: These steps do not affect the real computed value of 'height'."
}