{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-vertical-align",
        "http://www.w3.org/TR/CSS21/visudet.html#leading"
    ],
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "span",
                        "children": [
                            {
                                "text": "X",
                                "style": {}
                            },
                            {
                                "tag": "span",
                                "children": [
                                    {
                                        "text": "XXXXX XXXXX XXXXX XXXXX XXXXX",
                                        "style": {}
                                    }
                                ],
                                "style": {
                                    "margin_bottom": "1em",
                                    "width": "5em",
                                    "overflow_y": "hidden",
                                    "height": "4em",
                                    "color": "orange",
                                    "overflow_x": "hidden",
                                    "display": "inline-block"
                                }
                            },
                            {
                                "text": "X",
                                "style": {}
                            }
                        ],
                        "style": {
                            "background_position_x": "initial",
                            "background_attachment": "initial",
                            "background_origin": "initial",
                            "background_size": "initial",
                            "background_clip": "initial",
                            "background_repeat_y": "initial",
                            "background_color": "blue",
                            "color": "blue",
                            "background_image": "initial",
                            "background_position_y": "initial",
                            "background_repeat_x": "initial"
                        }
                    }
                ],
                "style": {
                    "font_weight": "normal",
                    "font_variant_caps": "normal",
                    "font_family": "Ahem",
                    "unicode_bidi": "embed",
                    "font_size": "20px",
                    "font_style": "normal",
                    "line_height": "1em",
                    "display": "block"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "unicode_bidi": "embed",
            "margin_top": "8px",
            "margin_right": "8px",
            "display": "block"
        }
    },
    "assert": "The 'vertical-align' property aligns an 'inline-block' elements' bottom margin edge with the baseline when 'overflow' is not set to 'visible'.",
    "matches": null
}