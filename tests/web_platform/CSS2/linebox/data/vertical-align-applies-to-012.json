{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-vertical-align",
        "http://www.w3.org/TR/CSS21/visudet.html#leading"
    ],
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "div",
                "children": [
                    {
                        "text": "X",
                        "style": {}
                    }
                ],
                "style": {
                    "background_position_x": "initial",
                    "font_variant_caps": "normal",
                    "width": "1in",
                    "font_size": "20px",
                    "background_clip": "initial",
                    "background_color": "orange",
                    "background_image": "initial",
                    "background_repeat_x": "initial",
                    "background_attachment": "initial",
                    "background_size": "initial",
                    "unicode_bidi": "embed",
                    "height": "1in",
                    "background_repeat_y": "initial",
                    "vertical_align": "bottom",
                    "background_origin": "initial",
                    "font_family": "Ahem",
                    "font_weight": "normal",
                    "font_style": "normal",
                    "line_height": "1em",
                    "color": "blue",
                    "background_position_y": "initial",
                    "display": "inline-block"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "unicode_bidi": "embed",
            "margin_top": "8px",
            "margin_right": "8px",
            "display": "block"
        }
    },
    "assert": "The 'vertical-align' property does not apply to elements with 'display' set to 'inline-block'.",
    "matches": "vertical-align-applies-to-001-ref.xht"
}
