{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#line-height"
    ],
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "div",
                        "children": [
                            {
                                "tag": "span",
                                "style": {
                                    "line_height": "5"
                                }
                            },
                            {
                                "text": "X",
                                "style": {}
                            }
                        ],
                        "style": {
                            "line_height": "1",
                            "background_color": "green",
                            "color": "white",
                            "unicode_bidi": "embed",
                            "display": "block"
                        }
                    },
                    {
                        "tag": "div",
                        "children": [
                            {
                                "text": "X",
                                "style": {}
                            }
                        ],
                        "style": {
                            "line_height": "5",
                            "width": "100%",
                            "position": "absolute",
                            "unicode_bidi": "embed",
                            "left": "0px",
                            "background_color": "red",
                            "top": "0px",
                            "z_index": "-1",
                            "display": "block"
                        }
                    }
                ],
                "style": {
                    "position": "relative",
                    "unicode_bidi": "embed",
                    "display": "block"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "unicode_bidi": "embed",
            "margin_top": "8px",
            "margin_right": "8px",
            "display": "block"
        }
    },
    "assert": "The line-height of an empty inline element influences the height of a line containing it with some other content",
    "matches": "empty-inline-003-ref.xht"
}
