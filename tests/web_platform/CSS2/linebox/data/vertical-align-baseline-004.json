{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-vertical-align",
        "http://www.w3.org/TR/CSS21/visudet.html#leading"
    ],
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "span",
                        "children": [
                            {
                                "text": "X",
                                "style": {}
                            },
                            {
                                "tag": "span",
                                "style": {
                                    "background_position_x": "initial",
                                    "width": "1in",
                                    "position": "relative",
                                    "background_origin": "initial",
                                    "background_clip": "initial",
                                    "background_color": "white",
                                    "background_image": "initial",
                                    "background_repeat_x": "initial",
                                    "background_attachment": "initial",
                                    "background_size": "initial",
                                    "top": "0.2em",
                                    "height": "1in",
                                    "background_repeat_y": "initial",
                                    "background_position_y": "initial",
                                    "display": "inline-block"
                                }
                            },
                            {
                                "text": "X",
                                "style": {}
                            }
                        ],
                        "style": {
                            "font_weight": "normal",
                            "font_variant_caps": "normal",
                            "font_family": "Ahem",
                            "font_size": "1in",
                            "font_style": "normal",
                            "line_height": "1em",
                            "color": "white"
                        }
                    }
                ],
                "style": {
                    "background_position_x": "initial",
                    "width": "3in",
                    "background_origin": "initial",
                    "background_clip": "initial",
                    "background_color": "black",
                    "background_image": "initial",
                    "background_repeat_x": "initial",
                    "background_attachment": "initial",
                    "background_size": "initial",
                    "unicode_bidi": "embed",
                    "background_repeat_y": "initial",
                    "background_position_y": "initial",
                    "display": "block"
                }
            },
            {
                "tag": "div",
                "children": [
                    {
                        "text": "XXX",
                        "style": {}
                    }
                ],
                "style": {
                    "background_position_x": "initial",
                    "width": "3in",
                    "background_origin": "initial",
                    "background_clip": "initial",
                    "background_color": "black",
                    "background_image": "initial",
                    "background_repeat_x": "initial",
                    "background_attachment": "initial",
                    "background_size": "initial",
                    "unicode_bidi": "embed",
                    "background_repeat_y": "initial",
                    "background_position_y": "initial",
                    "display": "block"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "unicode_bidi": "embed",
            "margin_top": "8px",
            "margin_right": "8px",
            "display": "block"
        }
    },
    "assert": "The 'vertical-align' property aligns an empty 'inline-block' elements' bottom margin edge with the baseline.",
    "matches": null
}