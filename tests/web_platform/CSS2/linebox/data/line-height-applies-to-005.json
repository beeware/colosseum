{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-line-height",
        "http://www.w3.org/TR/CSS21/visudet.html#leading"
    ],
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "div",
                        "children": [
                            {
                                "tag": "div",
                                "style": {
                                    "line_height": "2in",
                                    "unicode_bidi": "embed",
                                    "display": "table-column-group"
                                }
                            },
                            {
                                "tag": "div",
                                "children": [
                                    {
                                        "tag": "div",
                                        "style": {
                                            "background_position_x": "initial",
                                            "width": "1in",
                                            "background_origin": "initial",
                                            "background_clip": "initial",
                                            "background_color": "blue",
                                            "background_image": "initial",
                                            "background_repeat_x": "initial",
                                            "background_attachment": "initial",
                                            "background_size": "initial",
                                            "unicode_bidi": "embed",
                                            "height": "1in",
                                            "background_repeat_y": "initial",
                                            "background_position_y": "initial",
                                            "display": "table-cell"
                                        }
                                    }
                                ],
                                "style": {
                                    "unicode_bidi": "embed",
                                    "display": "table-row"
                                }
                            }
                        ],
                        "style": {
                            "table_layout": "fixed",
                            "unicode_bidi": "embed",
                            "display": "table"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "background_position_x": "initial",
                            "width": "1in",
                            "position": "absolute",
                            "background_origin": "initial",
                            "left": "1in",
                            "background_clip": "initial",
                            "background_color": "orange",
                            "background_image": "initial",
                            "background_repeat_x": "initial",
                            "background_attachment": "initial",
                            "background_size": "initial",
                            "unicode_bidi": "embed",
                            "top": "0px",
                            "height": "1in",
                            "background_repeat_y": "initial",
                            "background_position_y": "initial",
                            "display": "block"
                        }
                    }
                ],
                "style": {
                    "position": "relative",
                    "unicode_bidi": "embed",
                    "display": "block"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "unicode_bidi": "embed",
            "margin_top": "8px",
            "margin_right": "8px",
            "display": "block"
        }
    },
    "assert": "The 'line-height' property applies to elements with 'display' set to 'table-column-group'.",
    "matches": "line-height-applies-to-005-ref.xht"
}