{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-line-height",
        "http://www.w3.org/TR/CSS21/visudet.html#leading"
    ],
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "div",
                        "children": [
                            {
                                "text": "X",
                                "style": {}
                            }
                        ],
                        "style": {
                            "background_position_x": "initial",
                            "font_variant_caps": "normal",
                            "width": "1em",
                            "position": "relative",
                            "background_origin": "initial",
                            "font_family": "Ahem",
                            "font_size": "20px",
                            "background_clip": "initial",
                            "background_color": "black",
                            "background_image": "initial",
                            "background_repeat_x": "initial",
                            "font_weight": "normal",
                            "background_size": "initial",
                            "unicode_bidi": "embed",
                            "background_attachment": "initial",
                            "font_style": "normal",
                            "line_height": "inherit",
                            "background_repeat_y": "initial",
                            "background_position_y": "initial",
                            "display": "block"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "background_position_x": "initial",
                            "font_variant_caps": "normal",
                            "width": "1em",
                            "position": "absolute",
                            "font_size": "20px",
                            "background_clip": "initial",
                            "background_color": "black",
                            "background_image": "initial",
                            "background_repeat_x": "initial",
                            "background_attachment": "initial",
                            "background_size": "initial",
                            "unicode_bidi": "embed",
                            "height": "1in",
                            "background_repeat_y": "initial",
                            "background_origin": "initial",
                            "font_family": "Ahem",
                            "left": "1.1em",
                            "font_weight": "normal",
                            "top": "0px",
                            "font_style": "normal",
                            "line_height": "1em",
                            "background_position_y": "initial",
                            "display": "block"
                        }
                    }
                ],
                "style": {
                    "font_weight": "normal",
                    "font_variant_caps": "normal",
                    "width": "1em",
                    "position": "relative",
                    "font_family": "Ahem",
                    "unicode_bidi": "embed",
                    "font_size": "20px",
                    "font_style": "normal",
                    "line_height": "1in",
                    "display": "block"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "unicode_bidi": "embed",
            "margin_top": "8px",
            "margin_right": "8px",
            "display": "block"
        }
    },
    "assert": "The 'line-height' property set to 'inherit' uses the default line-height.",
    "matches": "line-height-006-ref.xht"
}
