{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-line-height",
        "http://www.w3.org/TR/CSS21/visudet.html#leading"
    ],
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "div",
                        "children": [
                            {
                                "text": "Filler Text",
                                "style": {}
                            }
                        ],
                        "style": {
                            "margin_bottom": "100px",
                            "font_size": "72pt",
                            "width": "100px",
                            "unicode_bidi": "embed",
                            "line_height": "100px",
                            "margin_top": "100px",
                            "background_color": "red",
                            "margin_left": "100px",
                            "margin_right": "100px",
                            "color": "green",
                            "height": "100px",
                            "display": "none"
                        }
                    }
                ],
                "style": {
                    "margin_bottom": "100px",
                    "margin_right": "100px",
                    "width": "100px",
                    "margin_left": "100px",
                    "unicode_bidi": "embed",
                    "margin_top": "100px",
                    "background_color": "green",
                    "color": "green",
                    "height": "100px",
                    "display": "block"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "unicode_bidi": "embed",
            "margin_top": "8px",
            "margin_right": "8px",
            "display": "block"
        }
    },
    "assert": "The 'line-height' property applies to elements with 'display' set to 'none'.",
    "matches": "line-height-applies-to-016-ref.xht"
}
