{
    "assert": "If an inline replaced element (like the svg element in this test) has no intrinsic width and no intrinsic ratio, then the used value of 'width' becomes 300px.",
    "matches": "../positioning/absolute-replaced-width-003c-ref.xht",
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "children": [
                            {
                                "tag": "svg:rect",
                                "style": {}
                            }
                        ],
                        "tag": "svg:svg",
                        "style": {
                            "vertical_align": "top",
                            "display": "inline"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "unicode_bidi": "embed",
                            "position": "relative",
                            "bottom": "300px",
                            "width": "300px",
                            "background_color": "green",
                            "display": "block",
                            "height": "300px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "unicode_bidi": "embed",
                    "width": "600px",
                    "display": "block",
                    "height": "300px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "unicode_bidi": "embed",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "margin_left": "8px",
            "display": "block",
            "margin_top": "8px"
        }
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-width"
    ]
}